{
  "name": "@healthcatalyst/cashmere-docs",
  "version": "2.1.0",
  "license": "Apache-2.0",
  "description": "Cashmere documentation website",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HealthCatalyst/Fabric.Cashmere.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run dev-mode && ng serve -o",
    "docs": "ts-node scripts/docs.ts && node scripts/generate-usage-docs.js",
    "search-prep": "ts-node scripts/search-prep.ts",
    "test": "ng test cashmere",
    "test:unit": "ng test cashmere --watch=false --code-coverage --no-progress",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint -c .eslintrc.js \"**/*.ts\"",
    "lint:prettier": "prettier --debug-check \"**/*.{ts,js,scss,css,md,json}\"",
    "build:lib": "ng build cashmere --configuration production",
    "postbuild:lib": "node scripts/postbuild.js",
    "build:user": "ng build user-guide --configuration production",
    "prebuild:examples": "ts-node scripts/build-examples.ts",
    "build:examples": "ng build cashmere-examples --configuration production",
    "build": "npm run clean && npm run release-mode && npm run build:lib && npm run build:examples && npm run search-prep && npm run build:user",
    "postbuild": "npm run copy-assets",
    "precopy-assets": "npm run docs",
    "copy-assets": "node scripts/guide-copy-assets.js",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "npm run release-mode && git add tsconfig.json",
    "precm": "ts-node scripts/ensure-staged.ts",
    "cm": "git-cz",
    "semantic-release": "semantic-release",
    "prettier": "prettier --write \"**/*.{ts,js,scss,css,md,json}\"",
    "release-mode": "node scripts/release-mode",
    "dev-mode": "node scripts/dev-mode",
    "postinstall": "npm run install:examples",
    "install:examples": "cd projects/cashmere-examples && npm ci",
    "new:example": "ts-node scripts/new-example.ts",
    "clean": "npx rimraf dist",
    "start-bs": "npm run dev-mode && ng serve -o --disable-host-check"
  },
  "dependencies": {
    "@angular/animations": "^17.3.8",
    "@angular/cdk": "^17.3.8",
    "@angular/common": "^17.3.8",
    "@angular/compiler": "^17.3.8",
    "@angular/core": "^17.3.8",
    "@angular/forms": "^17.3.8",
    "@angular/platform-browser": "^17.3.8",
    "@angular/platform-browser-dynamic": "^17.3.8",
    "@angular/platform-server": "17.3.8",
    "@angular/router": "^17.3.8",
    "@microsoft/applicationinsights-web": "^2.8.10",
    "@ng-select/ng-select": "^12.0.7",
    "@stackblitz/sdk": "^1.8.2",
    "@webcomponents/custom-elements": "^1.5.1",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.2",
    "markdown-it-container": "^3.0.0",
    "minisearch": "^2.5.1",
    "ngx-slider-v2": "^17.0.0",
    "notosans-fontface": "^1.3.0",
    "rxjs": "~6.6.0",
    "sugar": "^2.0.6",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.2",
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular-eslint/eslint-plugin": "^17.4.1",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.8",
    "@angular/language-service": "17.3.8",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^6.1.3",
    "@commitlint/travis-cli": "^9.0.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^7.3.0",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.4",
    "@types/prettier": "^1.15.2",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "chalk": "^2.3.0",
    "change-case": "^3.0.2",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.2.6",
    "coveralls": "^3.0.4",
    "cp": "^0.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "dgeni": "^0.4.14",
    "dgeni-packages": "^0.30.0",
    "echarts": "^5.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.3.0",
    "file-system": "^2.2.2",
    "fs-extra": "^6.0.1",
    "glob": "^10.3.4",
    "highlight.js": "^10.7.3",
    "husky": "^0.14.3",
    "inquirer": "^7.3.3",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "lint-staged": "^15.2.7",
    "markdown-it-named-headers": "0.0.4",
    "ng-packagr": "^17.3.0",
    "path": "^0.12.7",
    "prettier": "2.1.2",
    "progress": "^2.0.3",
    "punycode": "~1.4.1",
    "remove-markdown": "^0.3.0",
    "rimraf": "^4.2.0",
    "semantic-release": "^20.1.1",
    "ts-node": "^5.0.1",
    "typescript": "~5.4.5",
    "typescript-formatter": "^7.0.1",
    "uglify-js": "3.3.18",
    "yargs": "^15.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
