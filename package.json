{
    "name": "@healthcatalyst/cashmere-docs",
    "version": "0.0.3",
    "license": "Apache-2.0",
    "description": "Cashmere documentation website",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "test:unit": "ng test --single-run --code-coverage --no-progress",
        "lint": "tslint -p tsconfig.json",
        "build:lib": "node lib/build.js",
        "build:demo": "ng build --prod --no-progress",
        "prebuild": "npm run lint && npm run test:unit",
        "build": "npm run build:lib && npm run build:demo",
        "gh:404": "cp dist/index.html dist/404.html",
        "gh:cname": "echo $CASHMERE_URL > dist/CNAME",
        "publish:github": "npm run build:demo && npm run gh:404 && npm run gh:cname"
    },
    "dependencies": {
        "@angular/animations": "5.1.0",
        "@angular/common": "5.1.0",
        "@angular/compiler": "5.1.0",
        "@angular/core": "5.1.0",
        "@angular/forms": "5.1.0",
        "@angular/http": "5.1.0",
        "@angular/platform-browser": "5.1.0",
        "@angular/platform-browser-dynamic": "5.1.0",
        "@angular/platform-server": "5.1.0",
        "@angular/router": "5.1.0",
        "core-js": "^2.4.1",
        "font-awesome": "^4.7.0",
        "highlight.js": "^9.12.0",
        "markdown-it": "^8.4.1",
        "markdown-it-container": "^2.0.0",
        "npm-font-open-sans": "^1.1.0",
        "popper.js": "^1.12.9",
        "rxjs": "5.5.5",
        "zone.js": "^0.8.20"
    },
    "devDependencies": {
        "@angular/cli": "1.6.0",
        "@angular/compiler-cli": "5.1.0",
        "@angular/language-service": "5.1.0",
        "@types/jasmine": "~2.5.53",
        "@types/jasminewd2": "~2.0.2",
        "@types/moment": "^2.13.0",
        "@types/node": "6.0.92",
        "@types/popper.js": "^1.11.0",
        "chalk": "^2.3.0",
        "codelyzer": "~3.0.1",
        "cp": "^0.2.0",
        "glob": "^7.1.2",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "1.1.1",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "~2.0.0",
        "node-sass": "4.7.2",
        "protractor": "~5.1.2",
        "rollup": "^0.50.0",
        "rollup-plugin-license": "^0.5.0",
        "rollup-plugin-node-resolve": "^3.0.2",
        "rollup-plugin-sourcemaps": "^0.4.2",
        "shelljs": "^0.7.8",
        "ts-node": "~3.0.4",
        "tslint": "^5.8.0",
        "typescript": "2.4.2",
        "typescript-formatter": "^7.0.1",
        "uglify-js": "3.2.1",
        "webpack": "3.10.0"
    }
}
